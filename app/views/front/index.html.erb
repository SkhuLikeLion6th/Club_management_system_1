 <article class="article article_2">
   <div class="container">
        <!--소모임 & 학생회 -->
      <section class="section">
        <div class="align-center pt30 mt40">
          <h2>소모임</h2>
        </div>
        <div class="row mt30">
          <div class="col">
            <div class="row">
              <% @group_random.each do |x| %>
                <div class="col-2">
                  <% if user_signed_in? %> 
                    <a href="/small_groups/<%= x.id %>"> 
                      <div class="card dsply-inbl pl05 pr05 ml10 mr10 mb20" style="width: 11rem;">
                        <% if x.img_url.to_s == nil || x.img_url.to_s == "" %>
                          <img class="card-img-top small-index-main-img" src="/assets/small_groups/<%= x.id %>.png" alt="Card image cap" sytle="overflow: hidden;">
                        <% elsif %>
                          <img class="card-img-top small-index-main-img" src="<%=x.img_url%>" alt="Card image cap" sytle="overflow: hidden;">
                        <% end %>
                        <div class="dropdown-divider"></div>
                        <div class="card-body overflow-hddn">
                          <h5 class="card-title samll-index-main-title "><%= x.group_name %></h5>
                          <p class="card-text small-index-main-p"><%= x.introduce %></p>
