<header class="header-fluid">
  <div id="carouselExampleFade" class="carousel slide carousel-fade" data-ride="carousel">
    <div class="carousel-inner">
      <div class="carousel-item active ">
        <img class="d-block w-100" src="/assets/carousel-img/_ca_img_6.png" alt="First slide">
          <div class="carousel-caption d-none d-md-block">
            <a href="/board/manual/"><div style="height:200px; width: 800px; padding: 13rem;"><h2 class="car-h">&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;</h5></div></a>
            <p class="car-p"></p>
          </div>
      </div>
      <div class="carousel-item">
       <img class="d-block w-100" src="/assets/carousel-img/_ca_img_7.png" alt="Second slide">
        <div class="carousel-caption d-none d-md-block">
          
            <a href="/board/event/"><div style="height:200px; width: 800px; padding: 13rem;"><h2 class="car-h">&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;</h5></div></a>
            <p class="car-p">
        </div>
      </div>
      <div class="carousel-item">
        <img class="d-block w-100" src="/assets/carousel-img/_ca_img_3.png" alt="Second slide">
        <div class="carousel-caption d-none d-md-block">
         <a href="/board/investigation/" target="_blink"><div style="height:200px; padding-top: 13rem; color: #fff;" align="center"><h2 class="car-h">스쿠체크 설문조사를 써주세요!</h5></div>
            <p class="car-p" style="font-size: 2rem; color: #fff;">제발요.. <클릭!></p></a>
        </div>
      </div>
      <div class="carousel-item">
        <img class="d-block w-100" src="/assets/carousel-img/_ca_img_4.png" alt="Third slide">
        <div class="carousel-caption d-none d-md-block">
          <h2 class="car-h mt60" style="color: #4363a4; font-size: 2.5rem;">홍보 위치</h5>
          <p class="car-p" style="color: #4363a4; font-size: 2rem;">홍보가 올라올 위치입니다.</p>
        </div>
      </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleFade" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleFade" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
  <div class="header-bottom">
    <div class="container">
      <div class="row">
        <div class="col-4"></div>
        <div class="col-4 align-center">
          <div class="car-btn car-btn-1" data-target="#carouselExampleFade" data-slide-to="0"></div>
          <div class="car-btn car-btn-2" data-target="#carouselExampleFade" data-slide-to="1"></div>
          <div class="car-btn car-btn-3" data-target="#carouselExampleFade" data-slide-to="2"></div>
          <div class="car-btn car-btn-4" data-target="#carouselExampleFade" data-slide-to="3"></div>
        </div>
        <div class="col-4"></div>
      </div>
    </div>
  </div>
</header>
      
<!--club index-->
<article class="article">
  <div class="container">
    <!--공지 사항-->
    <section class="section">
      <div class="align-center">
        <div class="artcl-add-top">
          
          <div class="row">
            <div class="col-3">
              <img src="/assets/index/skhucheck_2.png" class="img-sz-l">
            </div>
            <div class="col-6">
              <h4 class="artcl-add-h">SKHU-CHECK 출시! 사용법을 익혀보세요.</h4>
            </div>
            <div class="col-3">
              <a href="/board/manual"><button type="button" class="artcl-add-btn">사용자 메뉴얼</button></a>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!--동아리 index & 홍보, 이벤트-->
    <section class="section">
      <div class="align-center pt30 mt40">
        <h2>동아리</h2>
      </div>
      <!--동아리 목록-->
      <div class="row mt30">
        <div class="col">
          <div class="row">
            <% @club_random.each do |x| %>
              <div class="col-3">
                <a href="/club/club_view/<%= x.id %>"> 
                  <div class="card" style="width: 16rem;">
                    <img class="card-img-top" src="<%= "/assets/clubs/" + x.id.to_s + ".jpg" %>" alt="Card image cap" style="width: 15rem; height: 15rem; margin: auto;">
                    <div class="card-body">
                      <h5 class="artcl-clubs-h"><%= x.club_name %></h5>
                      <p class="card-text artcl-clubs-p"><%= x.introduce %></p>
                    </div>
                  </div>
                </a>
              </div>
            <% end %>
          </div>  
        </div>  
      </div>
      <div class="align-center">
          <a href="/club/index"><button type="button" class="artcl-clubs-more" >더보기</button></a>
      </div>
    </section>
  </div>
</article>
